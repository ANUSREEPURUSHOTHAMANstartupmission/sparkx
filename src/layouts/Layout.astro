---
const { title } = Astro.props;
import '../style/global.css'

import Navbar from "../components/Navbar.astro";
import Navbarbottom from "../components/Navbar.svelte";


const description = "Kerala Startup Mission is the central agency of the Government of Kerala for entrepreneurship development and incubation activities in Kerala, India.";
const url = "https://startupmission.kerala.gov.in/";

---
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <link rel="icon" href="/favicon.png" type="image/x-icon" />
    <!-- Theme tittle --> 
    <title>KSUM | SparkX Incubation Program</title>  
    
    <!-- Theme style CSS -->         
    <link href="/css/style.css" rel="stylesheet"> 

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js" is:inline></script>    
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js" is:inline></script>    
    <![endif]-->
</head>
<body>

		<slot />

 <!-- Preloader -->  
    
    <!-- jQuery v3.3.1 -->
    <script src="/js/jquery-3.3.1.min.js" is:inline></script>    
    <!-- Bootstrap v4.0.0 -->
    <script src="/js/popper.min.js" is:inline></script>    
    <script src="/js/bootstrap.min.js" is:inline></script>      
    <!-- Extra Plugin -->
    <script src="vendors/animate-css/wow.min.js" is:inline></script>     
    <script src="vendors/owl-carousel/owl.carousel.min.js" is:inline></script>     
    
    <!-- Theme js / Custom js -->
    <script src="/js/theme.js" is:inline></script>     


        <footer class="footer_area">   
       <div class="container"> 
			 
			<div class="copy_right">
				<div class="row"> 
					<div class="col-md-6  wow slideInRight">
						<div class="flex items-end justify-end">
							<img src="/img/logosparkx.svg" class="w-48 md:w-52">
						</div>
					</div>   
					<div class="col-md-6 order-md-first  wow slideInLeft my-auto">
						<p>Copyrights 2025 <a href="https://startupmission.kerala.gov.in/" target="_blank">Kerala Startup Mission</a></p>
					</div> 
				</div>
			</div> 
       </div> 
    </footer>
</body>
</html> 